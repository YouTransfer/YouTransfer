{
	"PORT": 5000,

	"general": {
		"title": "IITP Transfer",
		"subtitle": "IIT Patna's file sharing portal",
		"baseUrl": "http://localhost:5000",
		"cleanupSchedule": "*/1 * * * *",
		"schedulerEnabled": false,
		"enableDownload": true
	},

	"dropzone": {
		"maxFilesize": "2000",
		"parallelUploads": 20,
		"forceFallback": false
	},

	"email": {
		"subject": "IITP Transfer",
		"transporter": "direct",
		"sendmailPath": "/usr/sbin/sendmail",
		"region": "eu-west-1",
		"rateLimit": 1
	},

	"storage": {
		"location": "local",
		"localstoragepath": "./uploads",
		"retention": 1,
		"retentionUnit": "weeks",
		"S3Region": "us-east-1",
		"S3SSLEnabled": "true"
	},

	"state": {}

}
